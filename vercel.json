{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "regions": ["iad1"],
    "functions": {
      "server.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }