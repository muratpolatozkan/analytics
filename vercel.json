{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 10,
          "memory": 1024
        }
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "regions": ["iad1"]
  }